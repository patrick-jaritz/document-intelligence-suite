{
  "functions": {
    "api/paddleocr.js": {
      "maxDuration": 30
    },
    "api/dots-ocr.js": {
      "maxDuration": 30
    },
    "api/crawl4ai.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/paddleocr/health",
      "destination": "/api/paddleocr.js?health=true"
    },
    {
      "source": "/api/paddleocr/ocr",
      "destination": "/api/paddleocr.js"
    },
    {
      "source": "/api/dots-ocr/health",
      "destination": "/api/dots-ocr.js?health=true"
    },
    {
      "source": "/api/dots-ocr/ocr",
      "destination": "/api/dots-ocr.js"
    },
    {
      "source": "/api/crawl4ai/health",
      "destination": "/api/crawl4ai.js?health=true"
    },
    {
      "source": "/api/crawl4ai/crawl",
      "destination": "/api/crawl4ai.js"
    }
  ]
}

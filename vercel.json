{
  "functions": {
    "api/paddleocr/ocr.js": {
      "maxDuration": 30
    },
    "api/dots-ocr/ocr.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/paddleocr/(.*)",
      "destination": "/api/paddleocr/ocr"
    },
    {
      "source": "/api/dots-ocr/(.*)",
      "destination": "/api/dots-ocr/ocr"
    }
  ]
}